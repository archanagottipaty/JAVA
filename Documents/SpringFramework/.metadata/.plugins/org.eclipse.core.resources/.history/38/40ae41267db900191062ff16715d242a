package ZooKeeper;

public class GiantBat extends mammal{
	int energyLevel = 300;
	
	public void fly() {
	
		System.out.println("Fly():shwoooooooooooooosh");}
	
	public void eatHumans() {
		energyLevel += 25;
		}
	
	public void attackTowns() {
		energyLevel -=100;
		System.out.println("attackTowns():Fire!Fire!" + energyLevel);}
	}
	

}
