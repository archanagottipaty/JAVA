package ObjectMaster;

public class Ninja extends Human{

	Ninja(){
		setStealth(10);
	}
	
	void steal(Human otherhuman){
		int value = otherhuman.getHealth()-getStealth();
		setHealth(value);
	}
		
	void getStealth(){
		System.out.println("Inside the Ninja.steal() function which does nothing");
	}
	
	void runAway() {
		System.out.println("Inside the Ninja.runAway() function which does nothing");
	}
}
